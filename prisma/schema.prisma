generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Post {
	id String @id @default(uuid())
	title String
	content String
	createdAt DateTime @default(now())
	updatedAt DateTime @default(now())
	updated Boolean @default(false)
}
